  UB   SCX_CPU1_MM_MemType     DESC="SCX CPU1 MM Memory Type for last command",
                               DISCRETE, DRANGE=(0,1,2,3,4,5), DLABEL=("None",
                               "RAM","EEPROM","MEM8","MEM16","MEM32")
