#include "ccsds_header.rdl"
!
  UB   SCX_CPU1_MM_CMDPC       DESC="SCX CPU1 MM Command Processed Counter",
                               UNITS=Counts
!
  UB   SCX_CPU1_MM_CMDEC       DESC="SCX CPU1 MM Command Error Counter",
